<div class="editor-field">
@Html.Telerik().DatePickerFor(model => model.TentativeDeliveryDate)
</div>
</td>

</tr>
<tr>
  @Html.EditorFor(model =>  model.DesignerContact)
</tr>
</table>

 <div>
  @(Html.Telerik().ScriptRegistrar())          
 </div> 